{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "rootDir": "./",
        "resolveJsonModule": true,
            
        "outDir": "./build",
        "preserveConstEnums": true,                       
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}
